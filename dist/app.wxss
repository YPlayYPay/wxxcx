page {
  height: 100%;
  width: 100%;
  overflow: hidden;
  background-color: #f9f9f9;
  font-family: -apple-system-font, 'Helvetica Neue', Helvetica, 'Microsoft YaHei', sans-serif;
}
view,
navigator,
text,
textarea {
  font: 14px tahoma, arial, 'hiragino sans gb', 'microsoft yahei', sans-serif;
}
textarea {
  overflow: auto;
}
.container {
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  height: 100%;
}
.row {
  margin-left: auto;
  margin-right: auto;
  margin-bottom: 40rpx;
}
.row .col {
  float: left;
  box-sizing: border-box;
  padding: 0 24rpx;
}
.row .col.s12 {
  width: 100%;
  margin-left: auto;
  left: auto;
  right: auto;
}
.row .col.s4 {
  width: 33.33333%;
  margin-left: auto;
  left: auto;
  right: auto;
}
.row:after {
  content: "";
  display: table;
  clear: both;
}
.color-white {
  background-color: #ffffff;
}
.h3 {
  font-size: 1.17em;
  margin: 0.83em 0;
}
.hr {
  width: 100%;
  height: 0;
  border-bottom: 2rpx solid #e0e0e0;
}
.flex-row-center {
  display: flex;
  justify-content: center;
  align-items: center;
}
.back-white {
  background-color: #fff;
}
