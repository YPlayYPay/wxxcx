  padding-bottom: 40rpx;
}
.button {
  cursor: pointer;
  box-sizing: border-box;
  height: 78rpx;
  display: inline-block;
  background-color: #ffffff;
  padding: 0 28rpx;
  text-align: center;
  transition: all .15s linear;
  border-radius: 2rpx;
  box-shadow: 0 2rpx 12rpx rgba(0, 0, 0, 0.117647), 0 2rpx 8rpx rgba(0, 0, 0, 0.117647);
}
.button text {
  color: #00bcd4;
  font-size: 26rpx;
  display: inline-block;
  height: 2.7em;
  line-height: 3em;
  overflow: hidden;
  cursor: pointer;
}
.editor {
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  margin: 40rpx 0;
  float: left;
  position: relative;
  width: 100%;
  border: 1px solid #cccccc;
}
.editor .editor-view1 {
  border-top-right-radius: 10rpx;
  border-top-left-radius: 10rpx;
  border-bottom: 2rpx solid #c6c6c6;
  background-color: #f7f7f7;
  padding-top: 10rpx;
  float: left;
  position: relative;
  width: 100%;
  height: 80rpx;
}
.editor .editor-view1 .editor-btn1 {
  margin-left: 20rpx;
  text-align: center;
  cursor: pointer;
  font-size: 24rpx;
  width: 96rpx;
  float: left;
  border: 2rpx solid #d9d9d9;
  background-color: #fff;
  border-radius: 4rpx;
  height: 24rpx;
  color: #666;
  padding: 20rpx 10rpx;
  line-height: 24rpx;
}
.editor .editor-view1 .editor-btn-2 {
  margin-left: -2rpx;
  background-color: #f7f7f7;
}
.editor .editor-view1 .editor-btn2 {
  color: #333;
  margin-left: 20rpx;
  text-align: center;
  cursor: pointer;
  font-size: 24rpx;
  border-radius: 6rpx;
  width: 96rpx;
  height: 40rpx;
  margin-right: 4rpx;
  float: left;
  padding: 12rpx 10rpx;
  border: 2rpx solid #d9d9d9;
  background-color: #fbfbfb;
  line-height: 40rpx;
}
.editor .editor-text-wrap {
  float: left;
  position: relative;
  width: 100%;
}
.editor .editor-text-wrap .editor-text {
  outline: none;
  margin: 0px;
  resize: none;
  width: 98%;
  height: 150px;
  border: 0px;
  color: #666464;
  overflow: auto;
}
