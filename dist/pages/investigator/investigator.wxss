.gat-wrap {
  margin: 10rpx 48rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}
.gat-wrap .gat-width {
  width: 100%;
}
.gat-wrap .gat-info {
  background-color: #eeeeee;
  border-radius: 10rpx;
  padding: 40rpx;
}
.gat-wrap .gat-btn {
  color: #00bcd4;
  font-size: 26rpx;
  display: inline-block;
  height: 2.7em;
  line-height: 3em;
  overflow: hidden;
  cursor: pointer;
}
.gat-wrap .gat-code {
  width: 90%;
  border-width: 2rpx 4rpx 2rpx 6rpx;
  border-style: solid;
  border-color: #d8d8d8 #ff6600 #d8d8d8 #ffc750;
  height: 160rpx;
  border-radius: 10rpx;
  padding: 24rpx 24rpx;
  margin-top: 30rpx;
}
.gat-wrap .gat-code .gat-code-row {
  margin: 10rpx 0 0;
}
.gat-wrap .gat-code .gat-code-row .gat-grey {
  color: #838383;
  font-size: 26rpx;
}
.gat-wrap .gat-code .gat-code-row .gat-yellow {
  color: #ffc750;
  font-size: 26rpx;
}
.gat-wrap .gat-code .gat-code-row .gat-black {
  color: #737373;
  font-size: 26rpx;
}
.gat-wrap .gat-code .gat-code-row .gat-light-black {
  color: #447a96;
  font-size: 26rpx;
}
.gat-wrap .gat-code .gat-code-row .gat-deep-white {
  color: #b0b0b0;
  font-size: 26rpx;
}
.gat-wrap .gat-text-color {
  color: #666666;
}
